programa {
	funcao inicio() {
		
		inteiro contador = 1
		inteiro numero = 0
		inteiro teste = 0
		
		enquanto (contador != 11)
		{
		    numero = contador * 9
		    contador = contador ++
		    teste = teste ++
		
		    escreva ("\n O resultado de 9 x " + teste + " é: " + numero)
		}
		
		
		
	}
}
